{
  "expo": {
    "name": "Fitness Rep Counter",
    "description": "Simple repetition counter for pullups, pushups and other fitness exercises. Includes visual progress charts.",
    "slug": "fitness-rep-counter",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "githubUrl": "https://github.com/marcelpanse/fitness-rep-counter",
    "android": {
      "package": "com.foobarbv.fitnessrepcounter",
      "permissions": []
    }
  }
}